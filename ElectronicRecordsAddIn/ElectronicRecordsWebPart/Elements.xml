<?xml version="1.0" encoding="utf-8"?>
<Elements xmlns="http://schemas.microsoft.com/sharepoint/">
  <ClientWebPart Name="ElectronicRecordsWebPart" Title="ElectronicRecordsWebPart Title" Description="ElectronicRecordsWebPart Description" DefaultWidth="300" DefaultHeight="200">

    <!-- Content element identifies the location of the page that will render inside the client web part
         Properties are referenced on the query string using the pattern _propertyName_
         Example: Src="~appWebUrl/Pages/ClientWebPart1.aspx?Property1=_property1_" -->
    <Content Type="html" Src="~appWebUrl/Pages/ElectronicRecordsWebPart.aspx?{StandardTokens}&amp;ListUrl=_ListUrl_&amp;wpId=_WPID_" />

    <!-- Define properties in the Properties element.
         Remember to put Property Name on the Src attribute of the Content element above. -->
    <Properties>
      <Property Name="ListUrl" Type="string" RequiresDesignerPermission="true" DefaultValue="" WebDisplayName="Url to the Electronic Records DocumentSet:" />
    </Properties>

  </ClientWebPart>
</Elements>
